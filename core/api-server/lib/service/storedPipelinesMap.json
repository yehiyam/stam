{
    "name": "myFlow",
    "nodes": [
        {
            "nodeName": "green",
            "algorithmName": "green-alg",
            "input": [
                "#@flowInput.files.links1"
            ]
        },
        {
            "nodeName": "yellow",
            "algorithmName": "yellow-alg",
            "input": [
                "@green"
            ]
        },
        {
            "nodeName": "black",
            "algorithmName": "black-alg",
            "input": [
                "@green",
                "@yellow"
            ]
        }
    ],
    "flowInput": {
        "files": {
            "link": "links-1",
            "links1": [
                "links-1",
                "links-2",
                "links-3",
                "links-4",
                "links-5"
            ],
            "links2": [
                "links-4",
                "links-5",
                "links-6"
            ]
        }
    },
    "webhook": {
        "progressHook": {
            "url": "http://webhook-stub-ui:3002/webhook/progress",
            "verbosityLevel": "debug"
        },
        "resultHook": {
            "url": "http://webhook-stub-ui:3002/webhook/result"
        }
    },
    "batchTolerance": 30
}